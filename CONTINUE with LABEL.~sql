BEGIN
  <<out_loop>>
  FOR outer_loop IN 1 .. 6 LOOP
    dbms_output.put_line('Outer loop, item=' || outer_loop);
    <<in_looop>>
    FOR inner_loop IN 1 .. 6 LOOP
      dbms_output.put_line('Inner loop, item=' || inner_loop);
      CONTINUE out_loop;
    END LOOP inner_loop;
  END LOOP outer_loop;
END;
